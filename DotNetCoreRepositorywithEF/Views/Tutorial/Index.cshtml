@model IEnumerable<DotNetCoreRepositorywithEF.Models.Tutorial>

@{
    ViewData["Title"] = "Title-Tutorial";
    Layout = "_Layout";
}

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td></td>
        </tr>
}
    </tbody>
</table>
